<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/hospital1"
    android:focusableInTouchMode="true"
    tools:context="com.medikeen.patient.RegistrationActivity" >

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:src="@drawable/login_bg"
        android:tint="#90000000" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/framlayoutr_margin_left"
        android:layout_marginRight="@dimen/framelayour_margin_right"
        android:layout_marginTop="@dimen/framelayou_margin_right"
        android:background="@android:color/transparent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <com.medikeen.views.MyTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center_horizontal"
                android:text="Profile"
                android:textColor="@android:color/white"
                android:textSize="@dimen/text_Size" />

            <com.medikeen.views.MyTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/text_view_margin_left"
                android:layout_marginTop="@dimen/text_view_margin_top"
                android:text="First Name"
                android:textSize="18dp"
                android:visibility="gone" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <com.medikeen.views.MyEditText
                    android:id="@+id/profile_editTextFirstName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:layout_marginTop="@dimen/edittext_margin_top"
                    android:background="@android:color/transparent"
                    android:hint="Enter First Name"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:singleLine="true"
                    android:textColor="@android:color/white"
                    android:textColorHint="#eeeeee" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/profile_editTextFirstName"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:background="#ffffff" />
            </RelativeLayout>

            <com.medikeen.views.MyTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/text_view_margin_left"
                android:layout_marginTop="@dimen/text_view_margin_top"
                android:text="Last name"
                android:textSize="18dp"
                android:visibility="gone" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone" >

                <com.medikeen.views.MyEditText
                    android:id="@+id/profile_editTextLastName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:layout_marginTop="@dimen/edittext_margin_top"
                    android:background="@android:color/transparent"
                    android:hint="Enter Last Name"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:singleLine="true"
                    android:textColor="@android:color/white"
                    android:textColorHint="#eeeeee" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/profile_editTextLastName"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:background="#ffffff" />
            </RelativeLayout>

            <com.medikeen.views.MyTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/text_view_margin_left"
                android:layout_marginTop="@dimen/text_view_margin_top"
                android:text="Email Id"
                android:textSize="18dp"
                android:visibility="gone" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <com.medikeen.views.MyEditText
                    android:id="@+id/profile_editTextEmailId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:layout_marginTop="@dimen/edittext_margin_top"
                    android:background="@android:color/transparent"
                    android:hint="Enter Email Id"
                    android:inputType="textEmailAddress"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:singleLine="true"
                    android:textColor="@android:color/white"
                    android:textColorHint="#eeeeee" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/profile_editTextEmailId"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:background="#ffffff" />
            </RelativeLayout>

            <com.medikeen.views.MyTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/text_view_margin_left"
                android:layout_marginTop="@dimen/text_view_margin_top"
                android:text="Password"
                android:textSize="18dp"
                android:visibility="gone" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone" >

                <com.medikeen.views.MyEditText
                    android:id="@+id/profile_editTextPassword"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:layout_marginTop="@dimen/edittext_margin_top"
                    android:background="@android:color/transparent"
                    android:hint="Enter Password"
                    android:inputType="textPassword"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:singleLine="true"
                    android:textColor="@android:color/white"
                    android:textColorHint="#eeeeee" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/profile_editTextPassword"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:background="#ffffff" />
            </RelativeLayout>

            <com.medikeen.views.MyTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/text_view_margin_left"
                android:layout_marginTop="@dimen/text_view_margin_top"
                android:text="Confirm Password"
                android:textSize="18dp"
                android:visibility="gone" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone" >

                <com.medikeen.views.MyEditText
                    android:id="@+id/profile_editTextConfirmPassword"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:layout_marginTop="@dimen/edittext_margin_top"
                    android:background="@android:color/transparent"
                    android:hint="Enter Password Again"
                    android:inputType="textPassword"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:singleLine="true"
                    android:textColor="@android:color/white"
                    android:textColorHint="#eeeeee" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/profile_editTextConfirmPassword"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:background="#ffffff" />
            </RelativeLayout>

            <com.medikeen.views.MyTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/text_view_margin_left"
                android:layout_marginTop="@dimen/text_view_margin_top"
                android:text="Address"
                android:textSize="18dp"
                android:visibility="gone" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <AutoCompleteTextView
                    android:id="@+id/profile_editTextAddress"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:layout_marginTop="@dimen/edittext_margin_top"
                    android:background="@android:color/transparent"
                    android:hint="Enter Address"
                    android:inputType="textMultiLine"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:singleLine="true"
                    android:textColor="@android:color/white"
                    android:textColorHint="#eeeeee" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/profile_editTextAddress"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:background="#ffffff" />
            </RelativeLayout>

            <com.medikeen.views.MyTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/text_view_margin_left"
                android:layout_marginTop="@dimen/text_view_margin_top"
                android:text="Contact No"
                android:textSize="18dp"
                android:visibility="gone" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <com.medikeen.views.MyEditText
                    android:id="@+id/profile_editTextContactNo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:layout_marginTop="@dimen/edittext_margin_top"
                    android:background="@android:color/transparent"
                    android:hint="Enter Contact Number"
                    android:inputType="number"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:singleLine="true"
                    android:textColor="@android:color/white"
                    android:textColorHint="#eeeeee" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/profile_editTextContactNo"
                    android:layout_marginLeft="@dimen/edittext_margin_left"
                    android:layout_marginRight="@dimen/edittext_margin_right"
                    android:background="#ffffff" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/edittext_margin_right"
                android:layout_marginRight="@dimen/edittext_margin_right"
                android:layout_marginTop="20dp"
                android:orientation="vertical" >

                <com.medikeen.views.MyButton
                    android:id="@+id/profile_changePassword"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/button_states_passenger"
                    android:text="Change Password"
                    android:textColor="#fff" />

                <com.medikeen.views.MyButton
                    android:id="@+id/profile_buttonsave"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#E53935"
                    android:text="Save"
                    android:textColor="#fff" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</FrameLayout>